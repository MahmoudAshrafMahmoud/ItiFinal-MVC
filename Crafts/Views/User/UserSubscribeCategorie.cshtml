
@{
    ViewBag.Title = "UserSubscribeCategorie";
    //Layout = "~/Views/Shared/_Main.cshtml";
}

<div style="width: 50px; height: 50px;">
    

        <form action="/user/AfterFollow" method="post">
        @foreach (var item in ViewBag.FollowVendors)
        {
            <div class="page__toggle">

                <label class="toggle">
                    <input class="toggle__input" type="checkbox" value="@item" checked name="SelectedVendors">
                    <span class="toggle__label">
                        <span class="toggle__text">@item</span>
                    </span>
                </label>
            </div>
        }
        <button type="submit">OK</button>
</form>


    







</div>

