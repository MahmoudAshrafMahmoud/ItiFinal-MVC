<aside class="single_sidebar_widget post_category_widget">
    <h4 class="widget_title mt-4 mb-3"> Followed Vendor</h4>
    <ul class="list cat-list ml-4">

        

        @foreach (var item in ViewBag.subs)
        {
            <li>
                <a  class="d-flex justify-content-between"
           href="@Url.Action("SelectedUser", "User",new {id= @item.User_Id})"> 
                <p>@item.FName @item.LName</p></a>

            </li>
        }

    </ul>
    <div class="br"></div>
</aside>

