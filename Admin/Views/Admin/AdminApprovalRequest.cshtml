@model List<DAL.Admin_Req_App_table>

@{
    ViewBag.Title = "AdminApprovalRequest";
}


<table class="table table-bordered">
   

    
    <thead>

    <tr>
        <td>Product ID</td>
        <td>Product Name</td>
        <td>Product Description</td>
        <td>Product Price</td>
        <td>State</td>
        <td>Vendor_id</td>
        <td>Approve</td>
        <td>Reject</td>
    </tr>

    </thead>
    
    <tbody>
    
    @foreach (var item in Model)
    {
        <tr>
                
            <td id="Req_id">
                    
                @item.Product_Id

            </td>
            <td>

                @item.Product_Name

            </td>
            <td>

                @item.Product_Description

            </td>
            <td>

                @item.Product_Price

            </td>
            <td>

                @item.Cat_id

            </td>
            <td>

                @item.Vendor_id

            </td>

            

        </tr>

        

    }

    </tbody>
</table>

